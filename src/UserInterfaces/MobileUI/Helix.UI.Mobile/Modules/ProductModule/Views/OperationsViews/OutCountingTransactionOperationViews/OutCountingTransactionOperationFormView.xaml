<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Helix.UI.Mobile.Modules.ProductModule.Views.OperationsViews.OutCountingTransactionOperationViews.OutCountingTransactionOperationFormView"
             xmlns:sharedViews="clr-namespace:Helix.UI.Mobile.Modules.BaseModule.SharedViews"
             x:Name="this"
           xmlns:viewModel="clr-namespace:Helix.UI.Mobile.Modules.ProductModule.ViewModels.OperationsViewModels.OutCountingTransactionOperationViewModels"
             x:DataType="viewModel:OutCountingTransactionOperationFormViewModel"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             
             Title="{Binding Title}"
             BackgroundColor="{StaticResource Primary}"
             Shell.TabBarIsVisible="False">
    <ContentPage.Behaviors>
        <toolkit:EventToCommandBehavior EventName="Loaded"
                                        Command="{Binding GetWarehouseCommand}" />
    </ContentPage.Behaviors>
    <sharedViews:ProductTransactionOperationFormContentView WarehouseList="{Binding WarehouseItems}"
                                                            GoToSuccessPageViewCommand="{Binding GoToSuccessPageViewCommand}"
                                                            ProductTransactionFormModel="{Binding ProductTransactionFormModel}"
                                                            TransactionTypeName="{Binding TransactionTypeName}"
                                                            Warehouse="{Binding Warehouse}"
                                                            >

    </sharedViews:ProductTransactionOperationFormContentView>
</ContentPage>